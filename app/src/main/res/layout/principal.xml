<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <FrameLayout
        android:id="@+id/frameLayoutBoton"
        android:layout_width="379dp"
        android:layout_height="230dp"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayoutMapa"
        android:layout_width="379dp"
        android:layout_height="230dp"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="@+id/frameLayoutBoton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/frameLayoutBoton"
        app:layout_constraintTop_toBottomOf="@+id/frameLayoutBoton">

    </FrameLayout>

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="104dp"
        android:text="Ir a hilos y corrutinas"
        app:layout_constraintEnd_toEndOf="@+id/frameLayoutMapa"
        app:layout_constraintStart_toStartOf="@+id/frameLayoutMapa"
        app:layout_constraintTop_toBottomOf="@+id/frameLayoutMapa" />
</androidx.constraintlayout.widget.ConstraintLayout>